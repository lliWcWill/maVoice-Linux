{
  "productName": "AquaVoice Linux",
  "version": "0.1.0",
  "identifier": "com.aquavoice.linux",
  "build": {
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "AquaVoice Linux",
        "width": 400,
        "height": 300,
        "resizable": true,
        "fullscreen": false,
        "alwaysOnTop": true,
        "decorations": true,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "category": "Utility"
  },
  "plugins": {
    "shell": {
      "open": true
    }
  }
}