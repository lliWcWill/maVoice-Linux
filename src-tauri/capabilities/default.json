{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default permissions for AquaVoice",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "shell:allow-open",
    "http:default",
    "global-shortcut:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "https://api.groq.com/**"
        }
      ]
    }
  ]
}